{
  "routes": [
    {
      "src": "/set/([^/]+)/([^/]+)",
      "dest": "/api/set?key=$1&pw=$2"
    },
    {
      "src": "/set/([^/]+)",
      "dest": "/api/set?key=$1"
    },
    {
      "src": "/get/([^/]+)/([^/]+)?",
      "dest": "/api/get?key=$1&pw=$2"
    },
    {
      "src": "/get/([^/]+)",
      "dest": "/api/get?key=$1"
    },
    {
      "src": "/([^/]+)/?([^/]+)?",
      "dest": "/api/get?key=$1&pw=$2"
    }
  ]
}
